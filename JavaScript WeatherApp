var weather = "Sunday";
if (weather == "Monday") {
  document.write("It is going to rain");
} else if (weather == "Tuesday") {
  document.write("It is going to snow");
} else if (weather == "wednesday") {
  document.write("It is going to be windy");
} else if (weather == "Thursday") {
  document.write("It is going to be cloudy");
} else if (weather == "Friday") {
  document.write("It is going to be snowing heavly 5.6ft");
} else if (weather == "Saturday") {
  document.write("It is going to be sunny all day");
} else if (weather == "Sunday") {
  document.write("It is going to be sunny with a chance of rain in the night");
}
